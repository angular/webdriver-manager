{
  "name": "webdriver-manager",
  "version": "12.1.8",
  "description": "A selenium server and browser driver manager for your end to end tests.",
  "scripts": {
    "build": "tsc && gulp copy",
    "format": "gulp format",
    "format-enforce": "gulp format:enforce",
    "gulp": "gulp",
    "jasmine": "jasmine",
    "prepack": "npm run format-enforce && tsc && gulp copy",
    "tsc": "tsc",
    "test": "npm run test-unit && npm run test-e2e",
    "pretest-unit": "npm run build",
    "test-unit": "jasmine",
    "pretest-e2e:update": "node built/lib/webdriver.js update --android --android-accept-licenses --avds none",
    "pretest-e2e:start": "node built/lib/webdriver.js start --detach --seleniumPort 4444 --android --appium-port 4723 --quiet",
    "pretest-e2e": "npm run build && npm run pretest-e2e:update && npm run pretest-e2e:start",
    "test-e2e": "jasmine JASMINE_CONFIG_PATH=e2e_spec/support/headless.json",
    "posttest-e2e": "node built/lib/webdriver.js shutdown"
  },
  "keywords": [
    "angular",
    "test",
    "testing",
    "protractor",
    "webdriver",
    "webdriverjs",
    "selenium",
    "selenium-webdriver"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/angular/webdriver-manager.git"
  },
  "bin": {
    "webdriver-manager": "bin/webdriver-manager"
  },
  "main": "built/lib/webdriver.js",
  "author": "Craig Nishina <craig.nishina@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.4.9",
    "chalk": "^1.1.1",
    "del": "^2.2.0",
    "glob": "^7.0.3",
    "ini": "^1.3.4",
    "minimist": "^1.2.0",
    "q": "^1.4.1",
    "request": "^2.87.0",
    "rimraf": "^2.5.2",
    "semver": "^5.3.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.29",
    "@types/chalk": "^0.4.28",
    "@types/form-data": "^0.0.33",
    "@types/glob": "^5.0.29",
    "@types/ini": "^1.3.28",
    "@types/jasmine": "^3.6.7",
    "@types/minimatch": "^2.0.28",
    "@types/minimist": "^1.1.28",
    "@types/node": "^14.14.35",
    "@types/q": "^1.5.4",
    "@types/request": "^2.48.5",
    "@types/rimraf": "^0.0.28",
    "@types/selenium-webdriver": "^2.53.35",
    "@types/semver": "^5.3.30",
    "@types/xml2js": "0.0.32",
    "clang-format": "^1.0.35",
    "gulp": "^4.0.0",
    "gulp-clang-format": "^1.0.23",
    "jasmine": "^3.6.4",
    "run-sequence": "^1.1.5",
    "selenium-webdriver": "~3.0.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=6.9.x"
  }
}
