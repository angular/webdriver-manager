{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "removeComments": false,
    "noImplicitAny": true,
    "outDir": "built/",
    "types": [
      "adm-zip", "chalk", "glob", "jasmine", "minimist",
      "node", "q", "request", "rimraf", "semver"
    ]
  },
  "exclude": [
    "built",
    "node_modules"
  ]
}
